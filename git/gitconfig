	br = branch

[color]
	branch = auto
	diff = auto
	status = auto

[color "branch"]
	current = green
	local = white
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = green
	changed = red
	untracked = red

[core]
	editor = vim
	packedGitLimit = 1g
	packedGitWindowSize = 1g
	bigFileThreshold = 1g
[safe]
	directory = *
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential]
	helper = store
